<h1>Create New Trip</h1>
<%= form_for @trip do |f| %>
  <%=f.label :name%>
  <%=f.text_field :name%>
  <%=f.label :location%>
  <%=f.text_field :location%>
  <%=f.label :start_date%>
  <%=f.date_field :start_date%>
  <%=f.label :end_date%>
  <%=f.date_field :end_date%>
  <br>
  <br>
  <%=f.submit class:"btn btn-primary"%>
<%end%>